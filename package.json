{
  "name": "codsen",
  "version": "1.0.0",
  "description": "Codsen website",
  "author": "Roy Revelt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codsen/codsen"
  },
  "parcelIgnore": [
    "images/*.*"
  ],
  "scripts": {
    "----------- DEV -----------": "",
    "start": "npm run dev",
    "dev": "npm run watch",
    "watch": "cross-env ELEVENTY_ENV=dev run-s clean:* build:assets && cross-env ELEVENTY_ENV=dev run-p serve watch:*",
    "watch:assets": "parcel watch ./src/assets/css/app.pcss ./src/assets/js/app.js --dist-dir ./bundle --no-source-maps --no-hmr",
    "serve": "date +%s > hash.txt && eleventy --serve --port=8080",
    "----------- PROD -----------": "",
    "build": "npm run build:eleventy && npm run search && npm run build:eleventy",
    "build:html": "date +%s > hash.txt && eleventy",
    "build:assets": "parcel build ./src/assets/css/app.pcss ./src/assets/js/app.js ./src/assets/js/interdeps.js ./src/assets/js/search-worker.js --dist-dir ./bundle --no-source-maps",
    "build:eleventy": "cross-env ELEVENTY_ENV=prod run-s clean:* build:assets build:html",
    "----------- SEED -----------": "",
    "seed": "cross-env ELEVENTY_ENV=seed run-s clean:* build:assets build:html && npm run search && run-p test:*",
    "----------- CLEAN -----------": "",
    "clean": "run-s clean:*",
    "clean:cache": "rimraf ./.cache",
    "clean:bundle": "rimraf ./bundle",
    "clean:dist": "rimraf ./dist",
    "----------- TEST -----------": "",
    "test": "npm run build:eleventy && npm run tests",
    "tests": "run-p test:*",
    "test:filters": "jest",
    "links": "linkinator ./dist --recurse --concurrency 1",
    "js:watch": "jest --watch",
    "----------- ETC -----------": "",
    "search": "node utils/scripts/fixSearchWorkerRoot.js"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.2",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@octokit/core": "^3.4.0",
    "@quasibit/eleventy-plugin-sitemap": "^2.1.4",
    "@tbranyen/jsdom": "^13.0.0",
    "array-shuffle": "^2.0.0",
    "axios": "^0.21.1",
    "countup.js": "^2.0.7",
    "cssnano": "^5.0.2",
    "cssnano-preset-advanced": "^5.0.1",
    "eleventy-plugin-markdown-shortcode": "^1.1.0",
    "fuse.js": "^6.4.6",
    "gray-matter": "^4.0.3",
    "html-minifier": "^4.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "markdown-escape": "^1.1.0",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-fence": "^0.1.3",
    "markdown-it-implicit-figures": "^0.10.0",
    "markdown-it-kbd": "^2.2.2",
    "matcher": "^4.0.0",
    "number-to-words": "^1.2.4",
    "p-map": "^4.0.0",
    "pacote": "^11.3.3",
    "parcel": "^2.0.0-beta.3.1",
    "parse-github-url": "^1.0.2",
    "postcss": "^8.2.15",
    "postcss-color-mix": "^1.1.0",
    "postcss-comment": "^2.0.0",
    "postcss-import": "^14.0.2",
    "postcss-mixins": "^7.0.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^6.0.3",
    "precss": "^4.0.0",
    "ranges-apply": "^5.0.16",
    "ranges-invert": "^4.0.16",
    "ranges-regex": "^4.0.16",
    "slugify": "^1.5.3",
    "string-strip-html": "^8.2.13",
    "string-unfancy": "^4.0.16",
    "title": "^3.4.3",
    "uslug": "^1.0.4",
    "what-input": "^5.2.10",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "delay-cli": "^1.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-row-num": "^1.5.12",
    "jest": "^26.6.3",
    "linkinator": "^2.13.6",
    "luxon": "^1.26.0",
    "npm-run-all": "^4.1.5",
    "parcel-resolver-ignore": "^1.0.1",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-cli": "^8.3.1",
    "postcss-easy-import": "^3.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.32.13"
  },
  "browserslist": [
    "last 1 Chrome versions",
    "last 1 Firefox versions",
    "last 1 Safari versions",
    "last 1 Edge versions"
  ]
}
